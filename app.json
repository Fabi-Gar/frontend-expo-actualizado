{
  "expo": {
    "name": "App Incendios Forestales",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "expo-notifications",
      "expo-web-browser"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuorg.incendios",
      "config": {
        "googleMapsApiKey": "TU_NUEVA_API_KEY_IOS"
      },
      "infoPlist": {
        "NSAppTransportSecurity": { "NSAllowsArbitraryLoads": true }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },

    "android": {
      "package": "com.garmen.incendios",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAUMi6n-ZXA4jn-07HdeOZm8FIzeF1ADKg"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS"
      ]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": { "projectId": "1b88fa29-0ad7-4035-8c50-50400f4978a5" }
    },
    "owner": "fabigar"
  }
}